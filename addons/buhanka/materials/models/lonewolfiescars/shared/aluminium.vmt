"VertexLitGeneric"
{
	"$basetexturetransform" "center 0 0 scale 0.1 1 rotate 0 translate 0 0"
	"$basetexture" "models/lonewolfiescars/shared/aluminium"
	"$bumpmap" "models/lonewolfiescars/shared/aluminium_bump"


	$nodecal "1"
	$normalmapalphaenvmapmask 1
  	$surfaceprop "metal"

   	$envmap "env_cubemap"
    	$envmaptint "[0.4 0.4 0.4]"
    	$envmapcontrast "1"
    	$envmapsaturation "1"
    	$envmapfresnel "1"

    	$phong "1"
    	$phongexponent "4"
    	$phongboost "6"
   	$phongfresnelranges "[ .2 .2 1]"

	"$colorfix" "{60 60 60}"
	"Proxies"
	{
		"Equals"
		{
			srcVar1 $colorfix
			resultVar	$color
		}
	}
}

{
	"$basetexturetransform" "center .3 .3 scale 280 280 rotate 0 translate 0 0"

	"$basetexture" "models/lonewolfiescars/shared/carbonfiber"
	"$bumpmap" "models/lonewolfiescars/shared/carbonfibernormal"

	"$nodecal" "1"
	"$normalmapalphaenvmapmask" 1

	"$phong" 1
	"$phongexponent" 2
	"$phongboost"	0.4
	"$phongtint" "[0.1 0.1 0.1]"
	"$phongfresnelranges" "[1 1 1]"

	"$envmap" "env_cubemap"
